{
  "expo": {
    "name": "WellMind",
    "slug": "WellMind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/WellMind.png",
    "scheme": "wellmind",
    "userInterfaceStyle": "automatic",
    "primaryColor": "#F48FB1",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/WellMind.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/WellMind.png"
    },
    "router": {
      "devTools": false
    },
    "plugins": [
      [
        "expo-router",
        {
          "sitemap": false
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/WellMind.png",
          "imageWidth": 512,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
